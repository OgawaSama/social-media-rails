<%= button_to "Sign out", destroy_user_session_path, method: :delete %> 
<%= button_to "Edit account", edit_user_registration_path %>
<%= button_to "Go to my profile", profile_path(id: current_user.profile.id) %>

<div class="fle flex-col items-center pt-24">
    <h1>My Feed</h1>

    <div class="mt-6 max-w-2xl w-full mx-auto flex items-center justify-between">
        <%= link_to "Create new post", new_post_path %>
    </div>

    <div class="max-w-5xl w-full grid gap-8 mt-8">
        <%= render partial: "post", collection: @posts %>
    </div>
</div>